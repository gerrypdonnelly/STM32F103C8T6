/**
 ******************************************************************************
Key PA0
LED PB2 APB2
 ******************************************************************************
 */

#include "stm32f103xb.h"



int main(void)
{
RCC->APB2ENR |=(1U<<3);//Enable clock to PB
//Set Mode of pin to output max speed 50Mhz
GPIOB_CRL[0]|=(1U<<8);
GPIOB_CRL[0]|=(1U<<9);
//Set CNF of pin to Output push pull
GPIOB->CRL[0]&=~(1U<<10);
GPIOB->CRL[0]&=~(1U<<11);
while(1)
{
	GPIOB->ODR ^=(1U<<2);
	for(int i =0; i < 1000000;i++){}
}
}

